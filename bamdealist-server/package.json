  "devDependencies": {
    "webpack-dev-middleware": "^1.5.1"
  },
  "repository": "https://haszari@bitbucket.org/haszari/bamdealist",
  "scripts": {
    "prepublish": "webpack --progress --colors",
    "normalise-db": "babel-node src/server/scripts/normalise-db --presets es2015",
    "clean": "rm -rf dist/",
    "build-www": "webpack",
    "build-server": "babel src/lib -d dist/lib --presets es2015; babel src/server -d dist/server --presets es2015",
    "build": "npm run build-www; npm run build-server",
    "start-db": "mongod --config /usr/local/etc/mongod.conf &",
    "start-production": "node dist/server/server.js",
    "start": "babel-node src/server/server.js --presets es2015",
    "import": "babel-node --presets es2015 src/server/scripts/import"
  },
